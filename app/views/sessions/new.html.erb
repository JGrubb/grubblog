<h2>Log in</h2>

<%= notice %>

<div id="login">
  <%= form_tag do %>
  
    <div class="field">
      <%= label_tag :email %>
      <%= text_field_tag :email, params[:email] %>
    </div>
    <div class="field">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class="submit">
      <%= submit_tag "Log in"%>
    </div>
  
  <% end %>
</div>